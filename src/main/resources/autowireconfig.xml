<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">
<context:annotation-config/>
    <bean class="com.corespring.autowire.Person" name="personInfo">
       <property name="name" value="Jiva Ram"/>
        <property name="age" value="25"/>
    </bean>

    <bean class="com.corespring.autowire.Person" name="personInfo2">
        <property name="name" value="Software Engineer"/>
        <property name="age" value="26"/>
    </bean>

<!--    <bean class="com.corespring.autowire.PersonInformation" name="person1" autowire="byType"/>-->
    <bean class="com.corespring.autowire.PersonInformation" name="person1" />


</beans>